if g:is_unix
    nnoremap ,vs :VimShell<CR>
endif
nnoremap ,cl :call SwitchCalendar()<CR>
nnoremap ,nt :NERDTreeToggle<CR>
nnoremap ,nu :call SwitchNumber()<CR>
nnoremap ,gi :Unite gista<CR>
" }}}

