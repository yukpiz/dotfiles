"echo substitute(text1, '\v([\>|\<|\!|\&|\*|\/|\\|\^|\+|\-|\=]\=*)', ' \1 ', 'g')
"echo substitute(text2, '\v([\>|\<|\!|\&|\*|\/|\\|\^|\+|\-|\=]\=*)', ' \1 ', 'g')
